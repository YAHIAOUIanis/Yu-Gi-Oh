{% extends 'base.html.twig' %}

{% block title %}{{ title }}{% endblock %}

{% block body %}
    <div class="container">
        <br>
        <div class="jumbotron">
            {{ form_start(form) }}

            {{ form_rest(form) }}
            <button class="btn btn-primary">Search</button>
            {{ form_end(form) }}
        </div>
        <br><br> <!-- A MODIFIER !-->

        {% for card in cards %}
            <a href="{{ path('manager.show', {'id': card.id}) }}"><img src={{ card.url }} alt="" height="320" width="210"></a>
        {% endfor %}

    </div>
{% endblock %}